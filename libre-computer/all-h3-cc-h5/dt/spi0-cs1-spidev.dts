/dts-v1/;
/plugin/;

/ {
	compatible = "allwinner,sun50i-h5";

	fragment@0 {
		target-path = "/aliases";
		__overlay__ {
			spi0 = "/soc/spi@01c68000";
		};
	};

	fragment@1 {
		target = <&spi0>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			spidev@0 {
				compatible = "spidev", "linux,spidev";
				reg = <0>;
				spi-max-frequency = <30000000>;
			};
			spidev@1 {
				compatible = "spidev", "linux,spidev";
				reg = <1>;
				spi-max-frequency = <30000000>;
			};
		};
	};
};
